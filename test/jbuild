(jbuild_version 1)

(executable
 ((name      mccs_test)
  (libraries (mccs))))

(include jbuild.inc)

(rule
 ((targets (jbuild.inc.gen))
  (deps    (gen_tests.ml (universe)))
  (action  (with-stdout-to ${@} (run ocaml gen_tests.ml)))))

(alias
 ((name   settests)
  (action (diff jbuild.inc jbuild.inc.gen))))
