(jbuild_version 1)

(library
 ((name              mccs)
  (cxx_names         (changed_criteria
                      constraint_generation
                      count_criteria
                      cudf_reductions
                      cudf_tools
                      glpk_solver
                      lexagregate_combiner
                      new_criteria
                      notuptodate_criteria
                      removed_criteria
                      mccscudf
                      mccs_stubs))
  (public_name       mccs)
  (cxx_flags         (:standard -I .))
;; for dynlink version:  (c_library_flags   (:standard -lglpk -lstdc++))
  (c_library_flags   (:standard -lz -lltdl))
  (library_flags     (-cclib -static-libgcc -cclib -Wl,-l:libstdc++.a,-l:libglpk.a))
  (wrapped           false)
  (libraries         (cudf))
  (no_dynlink)))
